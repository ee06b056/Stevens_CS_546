    <div id="templatemo_top_dishes">

      <h1>Area</h1>
     
         
         {{#factor}}
      <ul> 
       	  <li ><img src="{{url}}" alt="image" /></li>
           <li><h2>{{name}}({{area}})</h2></li> 
           <li><a href="comment?id={{name}}">Add Comment</a> </li> 
           
            </ul>
            <p> ingredients:</p>
            
            <ul>
                {{#each ingredients}}
                <li>{{this}}</li>
               {{/each}}
            </ul>   
           
          <p>Steps:</p>
        
          <ol>
                {{#each steps}}
              <li>{{this}}</li>
             {{/each}}
          </ol>
         

    {{/factor}}
   
   

      
        

        <div class="cleaner">&nbsp;</div>
    
   </div> 
    
